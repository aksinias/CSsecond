<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    </RelativeLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonTest2"
        android:text="buttonTest2"
        android:layout_marginTop="65dp"
        android:layout_below="@+id/relativeLayout"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:text="Currencies"
        android:id="@+id/textCurrencies"
        android:layout_marginLeft="47dp"
        android:layout_marginStart="47dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="105dp" />

    <ImageView
        android:layout_width="wrap_content"
        app:srcCompat="@drawable/liveratesbarrrr"
        android:id="@+id/imageView10"
        android:layout_height="70dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginBottom="56dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/relativeLayout5"
        android:layout_toEndOf="@+id/relativeLayout5">

        <ImageButton
            app:srcCompat="@drawable/newsbutton"
            android:scaleType="fitEnd"
            android:background="#00ffffff"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/ibNewsBlack" />

        <ImageButton
            app:srcCompat="@drawable/economiccalendarbuttongrey"
            android:id="@+id/ibEconCalBlack"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:cropToPadding="false"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="90dp"
            android:layout_marginStart="90dp" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/liveratesbuttonblue"
            android:id="@+id/ibLiveRates"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/ibEconCalBlack"
            android:layout_toEndOf="@+id/ibEconCalBlack" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@drawable/settingsbuttongrey"
            android:id="@+id/ibSettingsBlack"
            android:background="#00ffffff"
            android:scaleType="fitEnd"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

    </RelativeLayout>

</RelativeLayout>